{
  "name": "wishlist-api",
  "version": "0.0.0",
  "description": "A wishlist manager",
  "main": "index.js",
  "scripts": {
    "test": "bin/test.sh",
    "start": "export NODE_PATH=./lib && nodemon || node index.js",
    "docs": "node_modules/.bin/apidoc -i lib/routes/ -o docs/apidoc && node_modules/.bin/apidoc-markdown -p docs/apidoc -o docs/API.md",
    "db-build": "cd bin && ./db-build.sh",
    "db-destroy": "cd bin && ./db-destroy.sh",
    "db-rebuild": "cd bin && ./db-rebuild.sh",
    "db-seed": "cd bin && ./db-seed.sh",
    "compile-sass": "node_modules/.bin/node-sass --output-style compressed public/sass/materialize.scss public/dist/css/materialize.min.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robcalcroft/wishlist-api.git"
  },
  "author": "Rob Calcroft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robcalcroft/wishlist-api/issues"
  },
  "homepage": "https://github.com/robcalcroft/wishlist-api#readme",
  "dependencies": {
    "bcrypt": "^0.8.5",
    "body-parser": "^1.14.1",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "connect-pg-simple": "^3.1.0",
    "dotenv": "^1.2.0",
    "express": "^4.13.3",
    "express-session": "^1.11.3",
    "hat": "0.0.3",
    "jade": "^1.11.0",
    "md5": "^2.0.0",
    "morgan": "^1.6.1",
    "oauth2orize": "^1.0.1",
    "passport": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-oauth2-client-password": "^0.1.2",
    "pg": "^4.4.2",
    "rand-token": "^0.2.1",
    "rate-limiter": "^0.2.0",
    "request": "^2.67.0",
    "sql": "^0.60.0"
  },
  "devDependencies": {
    "apidoc": "^0.13.1",
    "apidoc-markdown": "^0.2.0",
    "babel-core": "^6.1.21",
    "babel-preset-es2015": "^6.1.18",
    "mocha": "^2.3.3",
    "mocha-jshint": "^2.2.5",
    "request": "^2.65.0"
  },
  "apidoc": {
    "name": "Wishlist Public API Docs",
    "version": "1.0.0",
    "description": "A vendor agnostic wishlist creator",
    "title": "Wishlist Public API Docs",
    "url": "http://wishlist.pw/api/<version>"
  }
}
